{
  "name": "vt-style",
  "version": "1.0.0",
  "description": "A Yaml to JSON conversion tool with a special focus on maobox style.",
  "bin": {
    "vt-style": "dist/cli.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p .",
    "watch": "tsc -w"
  },
  "author": "Geolonia (https://geolonia.com)",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "js-yaml": "^4.1.0",
    "meow": "9"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "^14.0.14",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^5.42.1",
    "webpack-cli": "^4.7.2"
  }
}
